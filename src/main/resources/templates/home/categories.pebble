<section class="container text-center my-4">
    {% for menuCategory in menuCategories %}
        <a href="/trueques-de-{{ menuCategory.slug }}.html" class="btn btn-lg btn-outline-secondary border-0 m-1">
            <i class="fa {{ menuCategory.icon }} display-4 d-block"></i>
            <h5 class="h6 m-0">{{ menuCategory.name }}</h5>
        </a>
    {% endfor %}
</section>

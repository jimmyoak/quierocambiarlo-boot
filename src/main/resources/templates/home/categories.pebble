<section class="container text-center my-4">
    {% for menuCategory in menuCategories %}
        <a href="/trueques-de-{{ menuCategory.slug }}.html" class="text-secondary d-inline-block m-3 text-decoration-none">
            <img src="{{ menuCategory.icon }}" alt="Icono de {{ menuCategory.name }}">
            <h5 class="h6 m-0">{{ menuCategory.name }}</h5>
        </a>
    {% endfor %}
</section>
